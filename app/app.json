{
  "expo": {
    "name": "app",
    "scheme": "servlix",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/servlix-high-resolution-logo.svg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/servlix-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#8e3fff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Outfit-Regular.ttf","./assets/fonts/Outfit-Medium.ttf"]
        }
      ]
    ]
    
  }
}
